        "focusBorder": "#546E7A",

        "list.activeSelectionBackground": "#455A64",

        "editorSuggestWidget.selectedBackground": "#37474F",
        "editorRuler.foreground": "#37474FA0",
        "editorIndentGuide.background": "#37474FA0",
        "editorLineNumber.foreground": "#37474FA0",
