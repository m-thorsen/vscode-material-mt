{
    "$schema": "vscode://schemas/color-theme",
    "name": "material",
    "type": "dark",
    "include": "./tokens.json",
    "colors": {
        // Terminal (Tango)
        "terminal.ansiWhite": "#D3DAE3",
        "terminal.ansiBlue": "#3465A4",
        "terminal.ansiBrightBlue": "#729FCF",
        "terminal.ansiCyan": "#06989A",
        "terminal.ansiBrightCyan": "#34E2E2",
        "terminal.ansiGreen": "#4E9A06",
        "terminal.ansiBrightGreen": "#8AE234",
        "terminal.ansiMagenta": "#75507B",
        "terminal.ansiBrightMagenta": "#AD7FA8",
        "terminal.ansiRed": "#CC0000",
        "terminal.ansiBrightRed": "#EF2929",
        "terminal.ansiYellow": "#C4A000",
        "terminal.ansiBrightYellow": "#FCE94F",
        // Blue
        "pickerGroup.foreground": "#5294e2",
        "inputOption.activeForeground": "#5294e2",
        "editorCursor.foreground": "#5294e2",
        "panelTitle.activeBorder": "#5294e2",
        // Highlight blues
        "editorSuggestWidget.highlightForeground": "#79ace8",
        "list.highlightForeground": "#79ace8",
        // Dark blues
        "list.focusBackground": "#404552",
        "list.activeSelectionBackground": "#404552",
        "inputOption.activeBackground": "#404552",
        "selection.background": "#5294e280",
        "editor.selectionBackground": "#5294e240",
        // Tabs
        "tab.activeBorderTop": "#5294e2",
        "tab.activeBackground": "#2f343f",
        // Badges & Buttons
        "badge.background": "#5294e2",
        "badge.foreground": "#fff",
        "button.background": "#5294e2",
        "button.hoverBackground": "#6ca4e6",
        "button.foreground": "#fff",
        "activityBarBadge.background": "#5294e2",
        "activityBarBadge.foreground": "#fff",
        "extensionButton.prominentBackground": "#5a8744",
        "extensionButton.prominentHoverBackground": "#88C36A",
        // Inputs
        "quickInput.foreground": "#95a1b1",
        "input.placeholderForeground": "#D3DAE380",
        "input.background": "#404552",
        // Base
        "editor.background": "#2f343f",
        "terminal.background": "#2f343f",
        "editorWidget.background": "#2f343f",
        // Darker
        "peekViewTitle.background": "#252a33",
        "peekViewEditor.background": "#252a33",
        // Lighter
        "panel.background": "#383C4A",
        "statusBar.noFolderBackground": "#383C4A",
        "activityBar.background": "#383C4A",
        "sideBar.background": "#383C4A",
        "statusBar.background": "#383C4A",
        "editorGroupHeader.tabsBackground": "#383C4A",
        "editorGroup.emptyBackground": "#383C4A",
        "statusBarItem.hoverBackground": "#383C4A",
        "editorGroupHeader.tabsBorder": "#383C4A",
        "peekViewResult.selectionBackground": "#383C4A",
        "peekViewResult.background": "#383C4A",
        "editorSuggestWidget.background": "#252a33",
        "editorHoverWidget.background": "#383C4A",
        // Foreground
        "editor.foreground": "#D3DAE3D0",
        "titleBar.inactiveForeground": "#D3DAE3D0",
        "sideBar.foreground": "#D3DAE3D0",
        "statusBar.foreground": "#D3DAE3D0",
        "statusBarItem.remoteForeground": "#D3DAE3D0",
        "activityBar.foreground": "#D3DAE3D0",
        "editorLineNumber.activeForeground": "#D3DAE3D0",
        "editorLineNumber.foreground": "#D3DAE335",
        "editorWhitespace.foreground": "#D3DAE335",
        // Ignore
        "gitDecoration.ignoredResourceForeground": "#D3DAE370",
        // Hover/active
        "pickerGroup.border": "#404552",
        "focusBorder": "#404552",
        "editor.selectionHighlightBorder": "#404552",
        "scrollbarSlider.activeBackground": "#404552",
        "scrollbarSlider.hoverBackground": "#404552",
        "menu.selectionBackground": "#404552",
        "list.hoverBackground": "#404552",
        // Misc
        "list.inactiveSelectionBackground": "#383C4A",
        "editorSuggestWidget.selectedBackground": "#383C4A",
        "editorIndentGuide.activeBackground": "#383C4A",
        "editorRuler.foreground": "#383C4A",
        "scrollbarSlider.background": "#383C4A",
        "dropdown.border": "#252a33",
        "dropdown.background": "#252a33",
        "titleBar.activeBackground": "#2f343f",
        "editorOverviewRuler.border": "#323843",
        "editorIndentGuide.background": "#323843",
        "titleBar.inactiveBackground": "#323843",
        // Borders
        "editor.lineHighlightBackground": "#21242c",
        "editorWidget.border": "#252a33",
        "titleBar.border": "#252a33",
        "editorGroup.border": "#252a33",
        "sideBar.border": "#252a33",
        "panel.border": "#252a33",
        "statusBar.border": "#252a33",
        "editorHoverWidget.border": "#252a33",
        "editorSuggestWidget.border": "#252a33",
        "editorGroupHeader.border": "#252a33",
        "peekView.border": "#252a33",
        "input.border": "#252a33",
        // Ignored/transparent
        "terminal.border": "#0000",
        "activityBar.border": "#0000",
        "statusBarItem.remoteBackground": "#0000",
        "sideBarSectionHeader.background": "#0000",
        "tab.inactiveBackground": "#0000",
    }
}
