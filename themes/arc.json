{
    "$schema": "vscode://schemas/color-theme",
    "name": "material",
    "type": "dark",
    "include": "./tokens.json",
    "colors": {
        // Chrome
        "activityBar.background": "#00000000",
        "sideBar.foreground": "#888",
        "statusBar.background": "#00000000",
        "statusBar.foreground": "#888",
        "editorLineNumber.foreground": "#ffffff20",
        "editorRuler.foreground": "#333",
        "editorIndentGuide.activeBackground": "#333",
        "editorIndentGuide.background": "#222",
        "editor.lineHighlightBackground": "#111",

        // Terminal
        "terminal.ansiWhite":"#9E9E9E",
        "terminal.ansiBlue":"#3761A4",
        "terminal.ansiBrightBlue":"#82AAFF",
        "terminal.ansiCyan":"#0097A7",
        "terminal.ansiBrightCyan":"#26C6DA",
        "terminal.ansiGreen":"#5a8744",
        "terminal.ansiBrightGreen":"#ADDF63",
        "terminal.ansiMagenta":"#9C27B0",
        "terminal.ansiBrightMagenta":"#CE93D8",
        "terminal.ansiRed":"#B71C1C",
        "terminal.ansiBrightRed":"#F07178",
        "terminal.ansiYellow":"#CC8820",
        "terminal.ansiBrightYellow":"#FFCB6B",
    }
}
